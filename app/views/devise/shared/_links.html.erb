<%- if controller_name != 'sessions' %>
  <span class="user-signin"><%= "もしかしてアカウントをお持ちですか？" %></span>
  <%= link_to "ログインする", new_session_path(resource_name), class:"login-signup" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <span class="user-signin"><%= "新規登録をしてWasmoを使おう！"%></span>
  <%= link_to "新規登録する", new_registration_path(resource_name), class:"login-signup" %><br />
<% end %>


